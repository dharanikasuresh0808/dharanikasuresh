    {
        for(int j=0;j<n;j++)
        {          
            if(i==0 || j==0 || i==n-1)
            {
            printf("* ");
            }
            else
            {
            printf("  ");
            }
            
        }
        printf("\n");
        
    }
    
    
    return 0;
}

